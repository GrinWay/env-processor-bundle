{
  "name": "grinway/env-processor-bundle",
  "type": "symfony-bundle",
  "version": "0.0.0",
  "description": "This bundle supplies commonly used symfony env processors",
  "keywords": [
    "grinway",
    "grinway-env-processor",
    "symfony",
    "bundle"
  ],
  "license": "MIT",
  "require-dev": {
    "symfony/phpunit-bridge": "^7.2",
    "phpunit/phpunit": "11.4",
    "symfony/test-pack": "^1.0"
  },
  "authors": [
    {
      "name": "Grigory Koblitskiy",
      "email": "grin180898@outlook.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "GrinWay\\EnvProcessor\\": "src/",
      "GrinWay\\EnvProcessor\\Tests\\": "tests/"
    }
  },
  "conflict": {
    "symfony/flex": "<1.13"
  },
  "extra": {},
  "minimum-stability": "stable",
  "require": {
    "symfony/framework-bundle": "^7.2",
    "symfony/runtime": "^7.2",
    "symfony/translation": "^7.2",
    "symfony/string": "^7.2",
    "symfony/yaml": "^7.2",
    "symfony/validator": "^7.2",
    "symfony/intl": "^7.2",
    "symfony/dotenv": "^7.2",
    "endroid/installer": "^1",
    "symfony/console": "^7.2"
  },
  "config": {
    "allow-plugins": {
      "symfony/runtime": true,
      "endroid/installer": true
    }
  }
}
